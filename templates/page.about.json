 \n \n  .timeline-main{\nbackground-color: #c09bf8;\npadding: 50px 0;\n\n}\n  .timeline {\n    width: 90%;\n    max-width: 950px;\n    margin: 0 auto; \/* Center align the parent div *\/\n    background-color: white;\n    color: #333;\n    padding: 30px 20px;\n    box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.2);\n    border-radius: 12px;\n    backdrop-filter: blur(10px);\n    border: 1px solid rgba(255, 255, 255, 0.2);\n  }\n  \n  .timeline ul {\n    list-style-type: none;\n    border-left: 2px solid lavender;\n    padding: 10px 5px;\n  }\n  \n  .timeline ul li {\n    padding: 20px 20px;\n    position: relative;\n    cursor: pointer;\n    transition: 0.5s;\n  }\n  \n  .timeline ul li span {\n    display: inline-block;\n    background-color: #c09bf8;\n    border-radius: 25px;\n    padding: 5px 20px;\n    font-size: 15px;\n    text-align: center;\ncolor:white;\n  }\n  \n  .timeline ul li .content h3 {\n    color:black; \/* Lavender color *\/\n    font-size: 17px;\n    padding-top: 5px;\n  }\n  \n  .timeline ul li .content p {\n    padding: 5px 0 15px 0;\n    font-size: 15px;\n  }\n  \n  .timeline ul li:before {\n    position: absolute;\n    content: '';\n    width: 10px;\n    height: 10px;\n    background-color: #b57edc; \/* Lavender color *\/\n    border-radius: 50%;\n    left: -11px;\n    top: 28px;\n    transition: 0.5s;\n  }\n  \n  .timeline ul li:hover {\n    background-color: rgba(255, 255, 255, 0.2);\n  }\n  \n  .timeline ul li:hover:before {\n    background-color: #d8bfd8; \/* Light lavender *\/\n    box-shadow: 0px 0px 10px 2px #d8bfd8;\n  }\n  \n  @media (max-width: 768px) {\n    .timeline {\n      width: 95%;\n      padding: 20px 15px;\n    }\n    .timeline ul li {\n      padding: 15px 15px;\n    }\n    .timeline ul li .content h3 {\n      font-size: 16px;\n    }\n  }\n  \n  @media (max-width: 480px) {\n    .timeline ul li .content h3 {\n      font-size: 15px;\n    }\n    .timeline ul li {\n      padding: 10px;\n    }\n  .timeline {\n      width: 90%;\n     \n 